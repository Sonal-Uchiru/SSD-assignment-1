    "scripts": {
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && jest",
        "build": "tsc",
        "prestart": "npm run build",
        "dev": "tslint -c tslint.json -p tsconfig.json --fix && ts-node-dev --respawn --transpile-only src/app.ts",
